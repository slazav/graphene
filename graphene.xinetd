# default: off
# description: Graphene database interface
service graphene
{
  disable         = yes
  type            = UNLISTED
  socket_type     = stream
  protocol        = tcp
  port            = 3001
  wait            = no
  user            = graphene
  nice            = 10
  rlimit_as       = 256M
  server          = /usr/bin/graphene
  server_args     = interactive
  log_type        = FILE /var/log/xinetd/graphene.log
  log_on_success  = HOST PID
  log_on_failure  = HOST
}
